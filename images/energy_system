digraph {
	electricity [color=lightgrey fixedsize=shape fontsize=10 height=0.3 shape=rectangle style=filled width=4.1]
	natural_gas [color=lightgrey fixedsize=shape fontsize=10 height=0.3 shape=rectangle style=filled width=4.1]
	wind [fontsize=10 shape=invtrapezium]
	pv [fontsize=10 shape=invtrapezium]
	rgas [fontsize=10 shape=invtrapezium]
	demand [fontsize=10 shape=trapezium]
	excess_bel [fontsize=10 shape=trapezium]
	pp_gas [fontsize=10 shape=rectangle]
	storage [fontsize=10 shape=rectangle style=rounded]
	wind -> electricity
	storage -> electricity
	pv -> electricity
	pp_gas -> electricity
	electricity -> demand
	electricity -> excess_bel
	electricity -> storage
	rgas -> natural_gas
	natural_gas -> pp_gas
}
